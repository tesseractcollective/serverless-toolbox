{
  "name": "serverless-toolbox",
  "version": "0.2.2",
  "description": "Reusable building blocks for AWS and Hasura",
  "main": "dist/index.js",
  "typings": "./dist/index.d.ts",
  "repository": "https://github.com/tesseract-collective/serverless-toolbox",
  "author": "Tesseract Collective",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "rm -rf ./dist/* && tsc"
  },
  "dependencies": {
    "aws-serverless-express": "^3.3.8",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@types/aws-serverless-express": "^3.3.3",
    "@types/express": "^4.17.6",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^13.11.0",
    "aws-sdk": "^2.674.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.9",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn build && git add dist/ && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,css,md}": "prettier --write"
  }
}
